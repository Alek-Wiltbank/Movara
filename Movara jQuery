<script>
jQuery(function($){
	$(document).ready(function(){
  		
		
		$('.tp-education-inactive').hide();
		
		
		$( "#carousel-education-healing-connection .et_pb_module_inner" ).click(function() {
			$('.tp-education-active').addClass('tp-education-inactive').hide().removeClass('tp-education-active');
			$('#tp-education-healing-connection').show().addClass('tp-education-active').removeClass('tp-education-inactive');
   		}); 
		
		$( "#carousel-education-mindfulness .et_pb_module_inner" ).click(function() {
			$('.tp-education-active').addClass('tp-education-inactive').hide().removeClass('tp-education-active');
