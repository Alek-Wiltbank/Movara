<script>
jQuery(function($){
	$(document).ready(function(){
  		
		
		$('.tp-recovery-inactive').hide();
		
		
		$( "#carousel-recovery-facials .et_pb_module_inner" ).click(function() {
			$('.tp-recovery-active').addClass('tp-recovery-inactive').hide().removeClass('tp-recovery-active');
			$('#tp-recovery-facials').show().addClass('tp-recovery-active').removeClass('tp-recovery-inactive');
   		}); 
		
		$( "#carousel-recovery-nails .et_pb_module_inner" ).click(function() {
			$('.tp-recovery-active').addClass('tp-recovery-inactive').hide().removeClass('tp-recovery-active');
			$('#tp-recovery-nails').show().addClass('tp-recovery-active').removeClass('tp-recovery-inactive');
   		}); 
		
		$( "#carousel-recovery-waxing-tinting .et_pb_module_inner" ).click(function() {
			$('.tp-recovery-active').addClass('tp-recovery-inactive').hide().removeClass('tp-recovery-active');
			$('#tp-recovery-waxing-tinting').show().addClass('tp-recovery-active').removeClass('tp-recovery-inactive');
   		}); 
		
		$( "#carousel-recovery-accupuncture .et_pb_module_inner" ).click(function() {
			$('.tp-recovery-active').addClass('tp-recovery-inactive').hide().removeClass('tp-recovery-active');
